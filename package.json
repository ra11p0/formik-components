{
  "name": "@ra11p0/formik-components",
  "version": "0.1.12-3",
  "private": false,
  "type": "module",
  "main": "./dist/esm/main.js",
  "module": "./dist/cjs/main.js",
  "types": "./dist/types/main.d.ts",
  "exports": {
    "import": "./dist/esm/main.js",
    "require": "./dist/cjs/main.js",
    "default": "./dist/esm/main.js"
  },
  "files": [
    "./dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ra11p0/formik-components.git"
  },
  "scripts": {
    "start": "vite",
    "vitebuild": "tsc && vite build",
    "build": "tsc",
    "build:production": "tsc --project tsconfig.esm.json & tsc --project tsconfig.cjs.json",
    "copyFiles": "cp ./src/lib/Components/common.css ./dist/cjs/Components & cp ./src/lib/Components/common.css ./dist/esm/Components",
    "clean": "rm -rf dist/*",
    "test": "vitest",
    "preview": "vite preview",
    "lint:code": "eslint ./src --ext .js,.ts,.jsx,.tsx"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-i18n-json": "^4.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-unused-imports": "^2.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "bootstrap": "^5.2.3",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "react-bootstrap": "^2.7.0",
    "react-datepicker": "^4.8.0"
  }
}
